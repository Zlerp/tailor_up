<%= simple_form_for(@article) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :kind %>
    <%= f.input :quantity %>
    <%= f.input :other %>
    <%= f.association :appointment %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
