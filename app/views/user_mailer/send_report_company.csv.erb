<%- headers = ['Appointment ID', 'Tailor Name', 'Name', 'Last Name', 'Email', 'Phone', 'Address', 'Zip', 'Date', 'Status'] -%>
<%= CSV.generate_line headers %>
<%- @company.tailors.each do |a| -%>
    <%  @appointments = a.appointments %>
    <% @appointments.each do |a| %>
  <%= CSV.generate_line([a.id + 10000, a.tailor.first_name, a.user.first_name, a.user.last_name, a.user.email, a.user.phone, a.address, a.zip, a.appointment_time, a.stages]) %>
<% end %>
<%- end -%>
