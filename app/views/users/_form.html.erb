<div class= "NewuSerImgback">
<%= simple_form_for @user, :html => {:multipart => true} do |f| %>
  <%= f.file_field :image %>
  <%= f.label :remote_image_url, "or image URL" %> <br />
  <%= f.text_field :remote_image_url %>
  <%= f.input :first_name %>
  <%= f.input :last_name %>
  <%= f.input :address %>
  <%= f.input :zip %>
  <%= f.input :phone %>
  <%= f.input :email %>
  <%= f.input :password %>
  <%= f.input :password_confirmation %>
  <%= f.button :submit %>
<% end %>
</div>
