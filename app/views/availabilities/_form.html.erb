
<%= form_for [@tailor, @availability] do |f| %>
  <%= f.label :start %>:
  <%= f.datetime_select :start, ampm: true, default: {minute: 0}, discard_minute: true, class: "datepicker" %><br />

  <%= f.label :end %>:
  <%= f.datetime_select :end, ampm: true, default: {minute: 0}, discard_minute: true  %><br />

  <%= f.submit %>
<% end %>
