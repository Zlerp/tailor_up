
<h3>This is the Sessions/new_sessions_tailor page</h3>

<h1>Tailor Log in</h1>
<%=form_tag sessions_login_tailor_path do %>
<p>
  <%# label_tag :email %>
  <%= text_field_tag :email, nil, :placeholder => 'example@TailorUp.com' %>
</p>
<p>
  <%# label_tag :password %>
  <%= password_field_tag :password, nil, :placeholder => 'password' %>
</p>
<p>
  <%= submit_tag 'Log in' %>
</p>

<% end %>
