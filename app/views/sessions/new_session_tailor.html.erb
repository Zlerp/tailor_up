
<div class="layer"></div>
<div id="loginBackground"></div>
<div class="context">

<span id="loginTitle"><h1>tailor</h1></span>
<%=form_tag sessions_login_tailor_path do %>
<p>
  <%# label_tag :email %>
  <%= text_field_tag :email, nil, :placeholder => 'example@TailorUp.com' %>
</p>
<p>
  <%# label_tag :password %>
  <%= password_field_tag :password %>
</p>
<p id="submitLogin">
  <%= submit_tag 'log in' %>
</p>

<% end %>
</div>
<%= render 'links' %>
