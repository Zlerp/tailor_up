<div class="loginFields">
  <p>
    <%= text_field_tag :email, nil, :placeholder => "example@emails.com" %>
  </p>
  <p>
    <%= password_field_tag :password, nil, :placeholder => 'password'%>
  </p>
</div>
<p id="submitLogin">
  <%= submit_tag 'log in', :class => "btn waves-effect waves-light deep-purple"%>
</p>
