
<h3>This is the Sessions/new_company page</h3>
<h1>Company Log in</h1>
<%=form_tag sessions_login_company_path do %>


<p>
  <%# label_tag :email %>
  <%= text_field_tag :email, nil, :placeholder => 'example@TailorUp.com' %>
</p>
<p>
  <%# label_tag :password %>
  <%= password_field_tag :password, nil, :placeholder => 'Password'  %>
</p>
<p>
  <%= submit_tag 'Log in' %>
</p>
<div class="">
  <%= link_to 'Sign Up', new_companies_path%>
</div>

<% end %>
